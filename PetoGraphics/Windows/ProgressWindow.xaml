        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="" Height="100" Width="308" WindowStartupLocation="CenterScreen" ResizeMode="NoResize" Closing="ProggressWindow_Closing">
    <Grid Background="#FF1B1B1C">
        <Label x:Name="infoText" Content="Copying images" VerticalContentAlignment="Top" Margin="10,0,10,10"/>
        <ProgressBar Name="progressBar" Minimum="0" Maximum="100" Margin="10,30,10,10"/>
    </Grid>
</Window>
